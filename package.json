{
  "name": "image-sticker",
  "version": "0.0.0",
  "description": "An extension to paste images like stickers on a web page. Supports moving, scaling, and rotating. Duplication and deletion are also easy. No uploading occurs even if you are online.",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production && web-ext build --source-dir dist --overwrite-dest",
    "analyze": "webpack --mode production --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsktschy/image-sticker.git"
  },
  "author": "dsktschy (https://github.com/dsktschy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsktschy/image-sticker/issues"
  },
  "homepage": "https://github.com/dsktschy/image-sticker#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.0.0",
    "husky": "^5.0.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "node-filter-async": "^2.0.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.3",
    "web-ext": "^5.5.0",
    "webextension-polyfill-ts": "^0.24.0",
    "webpack": "^5.21.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "dependencies": {
    "nanoid": "^3.1.20",
