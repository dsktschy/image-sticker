{
  "name": "sticker",
  "description": "An extension to put images on web pages like stickers",
  "version": "0.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "default_popup.html",
    "default_icon": {
      "128": "default_icon.png"
    }
  },
  "icons": {
    "128": "default_icon.png"
  }
}
